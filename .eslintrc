{
  "extends": [
    "eslint:recommended"
  ],
  "ecmaFeatures": {
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "no-undef":0m
    "quotes": [
      0,
      "single"
    ],
    "strict": [
      2,
      "never"
    ],
    "babel/generator-star-spacing": 1,
    "babel/new-cap": 1,
    "babel/no-await-in-loop": 1
  }
}